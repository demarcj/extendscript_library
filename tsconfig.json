{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "noEmit": true,           

    "strict": true,
    "skipLibCheck": true,

    "lib": ["ES5"],
    "downlevelIteration": false,
    "noEmitHelpers": true,

    "noUnusedLocals": false,
    "noUnusedParameters": true
  },
  "include": ["jsx/**/*.ts"]
}
